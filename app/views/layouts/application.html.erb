<!DOCTYPE html>
<html>
  <head>
    <title>FoodOnline</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render partial: "layouts/partials/application/header" %>
    <div class="min_body_height">
      <div class="container">
        <div class="row">
          <%= render partial: "layouts/partials/flash_messages", flash: flash %>
        </div>
      </div>      
      <%= yield %>
    </div>
      <%= render partial: "layouts/partials/application/footer" %>
    </div>
  </body>
</html>